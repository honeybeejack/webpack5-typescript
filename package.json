{
  "author": "Jack Yang <jack.yang@zoom.us>",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.2",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.15",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@mui/base": "^5.0.0-beta.14",
    "@svgr/webpack": "^8.1.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "babel-loader": "^9.1.3",
    "core-js": "^3.32.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.24.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "express": "^4.18.2",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "lodash": "^4.17.21",
    "prettier": "^3.0.3",
    "regenerator-runtime": "^0.14.0",
    "sass": "^1.66.1",
    "sass-loader": "^13.3.2",
    "stylelint": "^15.10.3",
    "stylelint-config-standard-scss": "^11.0.0",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
    }
  },
  "license": "MIT",
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "main": "index.js",
  "name": "webpack5-typescript",
  "repository": "git@github.com:ylkjick532428/webpack5-typescript.git",
  "scripts": {
    "build": "webpack --mode production --progress --hide-modules",
    "commit": "cz",
    "corp": "cross-env NODE_ENV=development && cross-env production=development && node corp.js  --corp",
    "dev": "webpack serve",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "https": "cross-env NODE_ENV=development && cross-env production=development && node corp.js --corp --https",
    "lint": "eslint --ext .js,.ts,tsx ./src",
    "stylelint": "npx stylelint \"**/*.css\"",
    "prepare": "husky install",
    "start": "cross-env NODE_ENV=development && cross-env production=development && node corp.js"
  },
  "version": "1.0.0"
}
